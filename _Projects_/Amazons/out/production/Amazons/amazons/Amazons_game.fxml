<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="amazons.AmazonsGame">
    <children>
        <GridPane fx:id="Board" onMousePressed="#getPieceCords" onMouseReleased="#getNewCords" prefHeight="500.0" prefWidth="500.0" />
      <Pane layoutX="500.0" prefHeight="200.0" prefWidth="200.0">
         <children>
            <Label fx:id="timeLabel" layoutX="14.0" layoutY="31.0" prefHeight="17.0" prefWidth="172.0" />
            <Label layoutX="14.0" layoutY="14.0" text="Time elapsed" />
            <Label layoutX="14.0" layoutY="48.0" prefHeight="17.0" prefWidth="27.0" text="Turn:" />
            <Label fx:id="turn" layoutX="49.0" layoutY="48.0" text="Label" />
            <Button layoutX="74.0" layoutY="100.0" mnemonicParsing="false" onAction="#startTimer" text="Button" />
            <Label layoutX="35.0" layoutY="146.0" text="Label" fx:id="xandy" />
            <Label fx:id="color" layoutX="126.0" layoutY="146.0" text="Label" />

         </children></Pane>
    </children>
</AnchorPane>
